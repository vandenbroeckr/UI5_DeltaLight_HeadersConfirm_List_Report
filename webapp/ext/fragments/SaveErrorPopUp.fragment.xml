<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:smartfield="sap.ui.comp.smartfield" xmlns="sap.m">
	<Dialog afterClose="afterClose" class="sapUiPopupWithPadding" id="saveDialog" title="{i18n>SAVE}">
		<MessageStrip class="sapUiResponsiveMargin" id="saveMessageStrip" showCloseButton="false" showIcon="true" type="Error"
			visible="false"></MessageStrip>
		<beginButton>
			<Button press="onOKPressed" text="{i18n>BUTTON_Ok}"/>
		</beginButton>
	</Dialog>
</core:FragmentDefinition>